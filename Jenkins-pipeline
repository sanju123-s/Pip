    agent any

    stages {
        stage('Checkout') {
            steps {
                git url: 'https://github.com/sanju123-s/Pip', branch: 'main'
            }
        }

        stage('Build') {
            steps {
                echo 'building...'
            }
        }

        stage('Test') {
            steps {
                echo 'Testing...'
            }
        }
    }
}
